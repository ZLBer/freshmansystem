<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="po.StcompleteinfoEntity" table="stcompleteinfo" schema="baodao">
        <id name="sno" column="sno"/>
        <property name="bedding" column="bedding"/>
        <property name="phoneNum" column="phoneNum"/>
        <property name="parentName" column="parentName"/>
        <property name="parentPhoneNum" column="parentPhoneNum"/>
        <property name="homeAddr" column="homeAddr"/>
        <property name="militaryClothing" column="militaryClothing"/>
        <property name="shoeNum" column="shoeNum"/>
        <property name="height" column="height"/>
        <property name="weight" column="weight"/>
        <property name="loan" column="loan"/>
        <property name="rTime" column="r_time"/>
    </class>
</hibernate-mapping>