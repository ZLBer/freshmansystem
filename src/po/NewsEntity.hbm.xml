<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="po.NewsEntity" table="news" schema="baodao">
        <id name="id" column="id"/>
        <property name="title" column="title"/>
        <property name="description" column="description"/>
        <property name="url" column="url"/>
        <property name="inputtime" column="inputtime"/>
        <property name="readpoint" column="readpoint"/>
        <property name="type" column="type"/>
        <property name="content" column="content"/>
        <property name="urlonly" column="urlonly"/>
        <property name="selected" column="selected"/>
    </class>
</hibernate-mapping>